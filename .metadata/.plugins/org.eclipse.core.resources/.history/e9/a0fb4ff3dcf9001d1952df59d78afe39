package ar.edu.unlp.info.oo2.facturacion_llamadas_refactory;

public class LlamadaInternacional extends Llamada {
	private static double factorCosto = 200;
	
	public LlamadaInternacional(String telefonoRemitente, double duracion) {
		super(telefonoRemitente, duracion);
	}
	
	@Override
	public double getFactorCosto() {
		return LlamadaInternacional.factorCosto;
	}

}
